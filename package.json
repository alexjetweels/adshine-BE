{
  "name": "Adshine-Backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": {
    "packages": [
      "./apps/*"
    ]
  },
  "scripts": {
    "prepublish": "rimraf dist",
    "build:core": "nest build core",
    "build:bot": "nest build bot",
    "build:scheduler": "nest build scheduler",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "concurrently --kill-others-on-fail \"yarn start:bot:dev\" \"yarn start:core:dev\" \"yarn start:scheduler:dev\"",
    "start:debug": "concurrently --kill-others-on-fail \"yarn start:bot:debug\" \"yarn start:core:debug\" \"yarn start:scheduler:debug\"",
    "start:prod": "concurrently --kill-others-on-fail \"yarn start:bot:prod\" \"yarn start:core:prod\" \"yarn start:scheduler:prod\"",
    "start:bot:dev": "nest start bot --watch",
    "start:bot:debug": "nest start bot --debug --watch",
    "start:bot:prod": "node dist/apps/bot/src/main.js",
    "start:core:dev": "nest start core --watch",
    "start:core:debug": "nest start core --debug --watch",
    "start:core:prod": "node dist/apps/core/src/main.js",
    "start:scheduler:dev": "nest start scheduler --watch",
    "start:scheduler:debug": "nest start scheduler --debug --watch",
    "start:scheduler:prod": "node dist/apps/scheduler/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/core/test/jest-e2e.json"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^3.1.1",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.4.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@prisma/client": "^5.19.1",
    "@ton/core": "^0.59.1",
    "@ton/crypto": "^3.3.0",
    "@ton/ton": "^15.1.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "big-integer": "^1.6.52",
    "big-json": "^3.2.0",
    "buffer": "^6.0.3",
    "bull": "^4.16.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "concurrently": "^9.0.1",
    "convert-pino-request-to-curl": "^0.0.29",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "libphonenumber-js": "^1.11.8",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "moment": "^2.30.1",
    "nestjs-cls": "^4.4.1",
    "passport": "^0.7.0",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.2.2",
    "prisma": "^5.20.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "timekeeper": "^2.3.1",
    "ton-crypto": "^3.2.0",
    "tonweb": "^0.0.66",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/big-json": "^3.2.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@lib/utils(|/.*)$": "<rootDir>/libs/utils/src/$1"
    }
  }
}
